        email = adepu.dinesh.a@gmail.com
[core]
        editor = emacs
        excludesfile = ~/.gitignore
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
